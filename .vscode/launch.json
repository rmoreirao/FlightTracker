{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Aspire AppHost (dotnet run)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build AppHost",
      "program": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/backend/src/FlightTracker.AppHost/FlightTracker.AppHost.csproj"
      ],
      "cwd": "${workspaceFolder}/backend/src/FlightTracker.AppHost",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": ".NET Aspire AppHost (Launch DLL)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build AppHost",
      "program": "${workspaceFolder}/backend/src/FlightTracker.AppHost/bin/Debug/net9.0/FlightTracker.AppHost.dll",
      "args": [],
      "cwd": "${workspaceFolder}/backend/src/FlightTracker.AppHost",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
